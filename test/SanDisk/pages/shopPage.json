{
    "Search Icon": {
        "type": "css",
        "selector": ".dr_icon.icon-search"
    },
    "Search Field": {
        "type": "css",
        "selector": ".ui-autocomplete-input"
    },
    "Search Button": {
        "type": "id",
        "selector": "dr_searchbutton"
    },
    "Products Menu": {
        "type": "css",
        "selector": "li.dropdown",
        "items": "li[id*='dr_catID']"
    },
    "Top Sellers Buy Now Button": {
        "isCollection": true,
        "type": "css",
        "selector": ".offer-text .dr_button"
    },
    "Basket Icon": {
        "type": "css",
        "selector": "i[class$='basket']"
    },
    "Product Name In Basket": {
        "isCollection": true,
        "type": "css",
        "selector": ".dr_prod-name"
    }
}