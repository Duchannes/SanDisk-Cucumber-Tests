{
    "Search Icon": {
        "type": "id",
        "selector": "searchIcon"
    },
    "Search Field": {
        "type": "css",
        "selector": ".ui-autocomplete-input"
    },
    "Search Button": {
        "type": "id",
        "selector": "dr_searchbutton"
    },
    "List of products by searching": {
        "isCollection": true,
        "type": "css",
        "selector": "a.dr_productName"
    },
    "Products Menu": {
        "type": "css",
        "selector": "li.dropdown"
    },
    "Type of Products": {
        "isCollection": true,
        "type": "css",
        "selector": "li[id*='dr_catID']"
    },
    "topSellersBuyBtn": {
        "isCollection": true,
        "type": "css",
        "selector": ".offer-text .dr_button"
    },
    "basketIcon": {
        "type": "css",
        "selector": "i[class$=\"basket\"]"
    },
    "prodNameInBasket": {
        "type": "css",
        "selector": ".dr_prod-name"
    }
}